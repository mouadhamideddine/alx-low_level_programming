#include <stdio.h>
/**
 *main - Entry point of the program
 *
 *description : print message
 *
 *Return : Always 0 (Success).
 */
int main(void){
  char Letter;
  int Count;
  for (count = 0; count < 26; count++)
    {
      Letter = 'a' + count;
      putchar(Letter);
    }
  putchar('\n');
  return (0)
}
